{
  "name": "semio-community",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev",
    "dev:site": "astro dev",
    "dev:cms": "vite dev --config vite.cms.config.mjs",
    "dev:no-drafts": "SHOW_DRAFTS=false astro dev",
    "dev:cms:proxy": "npx decap-server --port 8081 --allowed-hosts=\"http://localhost:4321,http://127.0.0.1:4321\"",
    "start": "npm run dev",
    "build": "npm run build:cms && npm run build:site && npm run build:search",
    "build:site": "astro build",
    "build:search": "pagefind --site dist",
    "build:cms": "npm run cms:config && npm run cms:copy-assets && npm run cms:preview",
    "preview": "astro preview",
    "lint": "biome lint .",
    "format": "npm run format:code && npm run format:imports",
    "format:code": "biome format . --write && prettier -w \"**/*\" \"!**/*.{md,mdx}\" --ignore-unknown --cache",
    "format:imports": "biome check --formatter-enabled=false --write",
    "check": "astro check",
    "verify:drafts": "node test/verify-drafts.mjs",
    "render:modules": "node scripts/render-configurator-modules.mjs",
    "cms:preview": "vite build --config vite.cms.config.mjs",
    "cms:copy-assets": "node scripts/copy-admin-assets.mjs",
    "cms:config": "node scripts/generate-decap-config.mjs",
    "content:import:collections-hub": "node scripts/import-content-hub-collections.mjs",
    "content:import:events-hub": "node scripts/import-content-hub-events.mjs",
    "content:import:all-hub": "npm run content:import:events-hub && npm run content:import:collections-hub",
    "content:sync:collections": "node scripts/sync-content-hub-collections.mjs",
    "content:sync:organizations": "node scripts/sync-content-hub-organizations.mjs",
    "content:sync:events": "node scripts/sync-content-hub-events.mjs",
    "content:sync:all": "npm run content:sync:organizations && npm run content:sync:events && npm run content:sync:collections"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.10",
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "^4.0.13",
    "@astrojs/sitemap": "^3.6.0",
    "@hugeicons/core-free-icons": "^1.2.1",
    "@hugeicons/react": "^1.1.1",
    "@iconify-json/hugeicons": "^1.2.3",
    "@iconify-json/mdi": "^1.2.2",
    "@iconify-json/solar": "^1.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@semio/ecosystem-content-schema": "file:../ecosystem-content-schema",
    "@semio/ecosystem-site-core": "file:../ecosystem-site-core",
    "@shikijs/transformers": "^1.25.1",
    "@solar-icons/react-perf": "^1.0.0",
    "@tailwindcss/vite": "^4.1.11",
    "astro": "^5.15.5",
    "astro-icon": "^1.1.5",
    "astro-robots-txt": "^1.0.0",
    "astro-webmanifest": "^1.0.0",
    "clsx": "^2.1.1",
    "cssnano": "^7.0.6",
    "decap-cms-app": "^3.9.0",
    "hastscript": "^9.0.0",
    "mdast-util-directive": "^3.0.0",
    "mdast-util-to-markdown": "^2.1.2",
    "mdast-util-to-string": "^4.0.0",
    "motion": "^12.23.22",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rehype-external-links": "^3.0.0",
    "rehype-pretty-code": "^0.14.0",
    "rehype-unwrap-images": "^1.0.0",
    "remark-directive": "^3.0.0",
    "satori": "0.12.0",
    "satori-html": "^0.3.2",
    "sharp": "^0.33.5",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.5",
    "@biomejs/biome": "^1.9.4",
    "@pagefind/default-ui": "^1.3.0",
    "@resvg/resvg-js": "^2.6.2",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "autoprefixer": "^10.4.21",
    "pagefind": "^1.3.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "0.14.1",
    "reading-time": "^1.5.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.7.2",
    "yaml": "^2.8.1"
  }
}
